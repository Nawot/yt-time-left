{
    "name": "yt-time-left",
    "description": "__MSG_description__",
    "version": "0.1",
    "version_name": "Init",
    "manifest_version": 2,

    "author": "Nawot",
    "homepage_url": "https://github.com/Nawot/yt-time-left",

    "default_locale": "en",

    "icons":
    {
        "1024": "icons/1024.png",
        "512": "icons/512.png",
        "256": "icons/256.png",
        "128": "icons/128.png",
        "64": "icons/64.png",
        "48": "icons/48.png",
        "24": "icons/24.png",
        "16": "icons/16.png",
        "8": "icons/8.png"
    },

    "applications":
    {
        "gecko":
        {
            "id": "Nawot001@gmail.com",
            "strict_min_version": "68.0"
        }
    },

    "content_scripts":
    [
        {
            "matches": ["https://www.youtube.com/watch*"],
            "js": ["yt-time-left.js"],
            "run_at": "document_end"
        }
    ]

}